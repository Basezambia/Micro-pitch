{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "functions": {
    "server.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/socketio",
      "destination": "/api/socketio"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}